<%= render "layouts/errors", :object => @contribution %>

<%= form_for [@story, @contribution] do |f| %>
  <%= f.label :image_url %>
  <%= f.text_field :image_url %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>
